{
  "manifest_version": 3,
  "name": "Bitbucket Buddy",
  "version": "1.0",
  "description": "Friends don't let friends struggle through Bitbucket Cloud",
  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.org/*"
      ],
      "js": ["default-to-files-changed-content-script.js"]
    }
  ],
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
